<?xml version="1.0"?>
<launch>

    <arg name="load_config" default="true"/>
    	
    <rosparam file="$(find injerrobot_grafting_pipeline)/config/parameters_baxter.yaml" command="load" if="$(arg load_config)"/>
        
    <node name="grafting" pkg="injerrobot_grafting_pipeline" type="test_baxter.py" output="screen" >
        <remap from="joint_states" to="robot/joint_states" />
    </node>

</launch>
